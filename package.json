{
  "name": "nose2floor",
  "version": "1.0.1",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "EXPO_WEB_USE_HERMES=false expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "EXPO_WEB_USE_HERMES=false expo start --web",
    "generate:icons": "node scripts/generate-icons.cjs",
    "generate:licenses": "npx license-kit report --format text --output public/licenses.txt --dm none && node -e \"require('fs').copyFileSync('public/licenses.txt','assets/licenses.txt')\"",
    "generate:demo-data": "node scripts/generate-demo-import.cjs && npx prettier --write demo/demo-import.json",
    "screenshots:insights": "node scripts/capture-insights-screenshots.cjs",
    "postinstall": "npm run generate:licenses || echo \"Skipping license generation (offline).\"",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-community/slider": "5.0.1",
    "@shopify/react-native-skia": "2.2.12",
    "expo": "~54.0.0",
    "expo-asset": "~12.0.12",
    "expo-clipboard": "~8.0.8",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-router": "~6.0.23",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-get-random-values": "~1.11.0",
    "react-native-mmkv": "~3.0.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "uuid": "~9.0.1",
    "zustand": "~5.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-native": "~5.4.3",
    "@testing-library/react-native": "~12.4.5",
    "@types/jest": "~29.5.12",
    "@types/react": "~19.1.10",
    "@types/uuid": "~9.0.8",
    "@typescript-eslint/eslint-plugin": "~7.18.0",
    "@typescript-eslint/parser": "~7.18.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "~8.56.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-react-hooks": "~4.6.2",
    "prettier": "~3.3.3",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.0",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  }
}
